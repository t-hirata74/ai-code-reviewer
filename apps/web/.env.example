# Database Configuration (if needed for NextAuth)
DATABASE_URL=postgres://postgres:postgres@localhost:5432/ai_code_reviewer_development

# Optional: Analytics
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id

# Optional: Error Reporting
SENTRY_DSN=your_sentry_dsn